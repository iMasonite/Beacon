		  Self.StatClampMultiplierLabel.Top = Self.WeightLabel.Bottom + Gap
		  Self.StatClampMultiplierField.Left = FieldsLeft
		  Self.StatClampMultiplierField.Top = Self.StatClampMultiplierLabel.Top
